{
  "files.associations": {
    "*.css": "tailwindcss",
    "justfile": "just"
  },
  "shellformat.effectLanguages": [
    "shellscript",
    "dockerfile",
    "dotenv",
    "hosts",
    "ignore",
    "gitignore",
  ],
  "[css][scss][less]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript][javascriptreact][typescript][typescriptreact]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode",
    "editor.tabSize": 2
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "off",
  "breadcrumbs.symbolPath": "on",
  "code-runner.clearPreviousOutput": true,
  "continue.enableTabAutocomplete": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSurroundingLines": 2, // 'Fira Code'
  "editor.fontFamily": "Source Code Pro,DroidSansM Nerd Font, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "350",
  "editor.formatOnSave": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.inlayHints.fontFamily": "Indie Flower",
  "editor.inlayHints.fontSize": 14,
  "editor.inlineSuggest.enabled": true,
  "editor.letterSpacing": 1.2,
  "editor.lightbulb.enabled": "off",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.sectionHeaderLetterSpacing": 0,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.lintTask.enable": true,
  "eslint.useESLintClass": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.js": "",
    "*.jsx": "",
    "*.ts": "",
    "*.tsx": "",
    "configs": "*.ts, *.js, *.cjs, *.yaml, *.json, .*, *.toml, *.lockb, *.lock, dockerfile",
    "package.json": "",
    "tsconfig.json": ""
  },
  "explorer.openEditors.visible": 1,
  "files.exclude": {
    "**/*.aux": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.gz": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/__pycache__": true,
    ".next/": true,
    ".svelte-kit/": true,
    ".vercel/": true,
    ".vscode/": true,
    "build/": true,
    "next-env.d.ts": true,
    "node_modules/": true
  },
  "files.watcherExclude": {
    "node_modules/": true
  },
  "git-graph.commitDetailsView.location": "Docked to Bottom",
  "git-graph.dialog.addTag.pushToRemote": true,
  "git-graph.dialog.merge.noCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true,
  "git.mergeEditor": true,
  "git.openDiffOnClick": false,
  "git.openRepositoryInParentFolders": "always",
  "git.suggestSmartCommit": false,
  "git.terminalGitEditor": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "javascript.suggestionActions.enabled": false,
  "json.schemaDownload.enable": true,
  "latex-workshop.view.pdf.color.dark.backgroundColor": "black",
  "latex-workshop.view.pdf.color.dark.pageColorsBackground": "#1e1e1e",
  "latex-workshop.view.pdf.color.dark.pageColorsForeground": "white",
  "latex-workshop.view.pdf.color.light.backgroundColor": "",
  "latex-workshop.view.pdf.color.light.pageColorsBackground": "white",
  "Lua.completion.showWord": "Disable",
  "Lua.completion.workspaceWord": false,
  "Lua.workspace.library": [
    "/Users/ken/.hammerspoon/Spoons/EmmyLua.spoon/annotations",
    "/Users/ken/Documents/Software/DevBox/dotfiles/wezterm/types"
  ],
  "remoteHub.commitDirectlyWarning": "off",
  "scm.providerCountBadge": "auto",
  "scm.showActionButton": false,
  "search.exclude": {
    "**/#current-cloud-backend": true,
    "**/node_modules": false,
    "**/node_modules/": true,
    "**/src/opt/": true
  },
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.untrustedFiles": "open",
  "stylelint.useLocal": true,
  "svg.preview.mode": "svg",
  "tailwind-fold.autoFold": false,
  "tailwind-fold.foldedText": "css",
  "tailwind-fold.unfoldIfLineSelected": true,
  "tailwindcss-highlight.customUtilitiesConfig": {},
  "tailwindCSS.experimental.classRegex": [
    "{([^}]*)},",
    "'([^']*)'"
  ],
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.fontFamily": "Source Code Pro, DroidSansM Nerd Font Mono, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.letterSpacing": 3,
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "args": [
        "-l"
      ],
      "path": "zsh"
    }
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "background": "#c46f00",
      "foreground": "#ffffff",
      "icon": "bookmark",
      "iconColour": "#c46f00",
      "type": "line"
    }
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.revealIfOpen": true,
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "workbench.iconTheme": null,
  "workbench.layoutControl.enabled": false,
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.colorCustomizations": {
    // BRACKETS
    "editorBracketHighlight.foreground1": "#8f6f00",
    "editorBracketHighlight.foreground2": "#0c8a00",
    "editorBracketHighlight.foreground3": "#a23cb8",
    "editorBracketHighlight.foreground4": "#8f6f00",
    "editorBracketHighlight.foreground5": "#0c8a00",
    "editorBracketHighlight.foreground6": "#a23cb8"
  },
  "editor.tokenColorCustomizations": {
    // Manage everything except brackets in textmate
    // Brackets managed above 
    "textMateRules": [
      {
        "scope": [
          "keyword.operator", // =, +, -, !, ..., &&
          "punctuation", // ; , "
        ],
        "settings": {
          "foreground": "#819e00"
        }
      },
      {
        "scope": [
          "constant.numeric", // numbers
          "constant.language", // null
        ],
        "settings": {
          "foreground": "#00b162"
        }
      },
      {
        "scope": [
          "comment", // comment
          "punctuation.definition.comment" // //
        ],
        "settings": {
          "foreground": "#0c8a00"
        }
      },
      {
        "scope": [
          "string",
        ],
        "settings": {
          "foreground": "#00a5e2"
        }
      },
      {
        "scope": [
          "support.class", // jsx
          "punctuation.definition.tag", // jsx
          "entity.name.tag.tsx"
        ],
        "settings": {
          "foreground": "#00b294"
        }
      },
      {
        "scope": [
          "entity.name.function", // function
          "entity.name.type", // decorator 
        ],
        "settings": {
          "foreground": "#00d0df"
        }
      },
      {
        "scope": [
          "storage.type", // const, let keywords
          "keyword.control", // if, return keywords
        ],
        "settings": {
          "foreground": "#00b9ff"
        }
      },
      {
        "scope": [
          "variable", // variable
          "variable.other.constant", // constant
          "entity.name.namespace", // argparse
        ],
        "settings": {
          "foreground": "#00b9ff"
        }
      },
      {
        "scope": [
          "support.variable", // .length
          "meta.attribute", // attribute
          "variable.other.property", // property
          "support.type.property-name" // json property
        ],
        "settings": {
          "foreground": "#00b9ff"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name", // jsx-attribute
        ],
        "settings": {
          "foreground": "#00b9ff"
        }
      }
    ]
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "projectManager.sortList": "Saved",
  "git.path": "/Users/ken/.local/share/mise/installs/git/latest/bin/git",
}