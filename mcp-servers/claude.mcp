{
  "mcpServers": {
    "Obsidian": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "mcp-obsidian",
        "/Users/ken/Documents/obsidian/Vault"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "mcp-sequentialthinking-tools": {
      "command": "bunx",
      "args": [
        "mcp-sequentialthinking-tools"
      ],
      "env": {
        "MAX_HISTORY_SIZE": "1000"
      }
    },
    "github": {
      "command": "github-mcp-server",
      "args": [
        "stdio"
      ]
    },
    "playwright": {
      "command": "bunx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "mcp-server-chart": {
      "command": "bunx",
      "args": [
        "@antv/mcp-server-chart"
      ]
    }
  }
}

