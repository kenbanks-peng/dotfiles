#!/usr/bin/env zsh
# [MISE] description="Nix profile list packages with versions"
# [MISE] dir="{{cwd}}"

# Extract package-version from nix profile list store paths

nix profile list 2>/dev/null | awk '/^Store paths:/ { print $NF }' | while IFS= read -r path; do
  base="${path##*/}"
  echo "${base:33}"
done | sort
